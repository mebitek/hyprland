account cm_mebitek
host mail.mebitek.com
port 465
from claudio.melis@mebitek.com
user claudio.melis@mebitek.com
passwordeval "pass claudio.melis@mebitek.com"
auth on
tls on
tls_trust_file	/etc/ssl/certs/ca-certificates.crt
logfile /home/mebitek/.config/msmtp/msmtp.log
tls_starttls off

